    <script>
        function toggleSubButtons(vertical) {
            var currentVertical = document.querySelector('.' + vertical);
            var currentSubButton = currentVertical.querySelector('.sub-button');
            if (currentVertical.classList.contains('clicked')) {
                currentVertical.classList.remove('clicked');
                currentSubButton.style.display = 'none';
            } else {
                var verticals = document.querySelectorAll('.vertical');
                verticals.forEach(function(v) {
                    v.classList.remove('clicked');
                });
                var subButtons = document.querySelectorAll('.sub-button');
                subButtons.forEach(function(button) {
                    button.style.display = 'none';
                });
                currentVertical.classList.add('clicked');
                currentSubButton.style.display = 'block';
            }
        }
    </script>
</body>
</html>


