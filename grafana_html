    <script>
        function toggleDashboardLinks(dashboardId, buttonId) {
            var dashboardLinks = document.getElementById(dashboardId + "Links");
            dashboardLinks.classList.add("active");
            var allDashboardLinks = document.querySelectorAll(".dashboard-links");
            allDashboardLinks.forEach(function(link) {
                if (link.id !== dashboardId + "Links" && link.classList.contains("active")) {
                    link.classList.remove("active");
                }
            });
            var allButtons = document.querySelectorAll(".button");
            allButtons.forEach(function(button) {
                if (button.id !== buttonId && button.classList.contains("active")) {
                    button.classList.remove("active");
                }
            });
            var clickedButton = document.getElementById(buttonId);
            clickedButton.classList.add("active");
        }

        function openDashboard(linkId) {
            var url = ""; // Add the URL of the dashboard here
            window.open(url, '_blank');
        }
    </script>
</body>
</html>


